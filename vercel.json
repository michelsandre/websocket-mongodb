{
  "version": 2,
  "builds": [
    {
      "src": "./server/*.*",
      "use": "@vercel/node"
    },
    {
      "src": "./public/*.*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "./public/index.html"
    },
    {
      "src": "/login",
      "dest": "./public/login/index.html"
    }
  ]
}
